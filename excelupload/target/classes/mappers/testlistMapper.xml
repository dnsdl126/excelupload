<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.excelupload.persistence.testListDAO">
	<select id="list" resultType ="com.excelupload.domain.testListDTO">
		select * from "testS200".school
	</select>

	
	 <insert id="insertExcel" parameterType="com.excelupload.domain.testListDTO">
        INSERT INTO "testS200".school ( snum, sname, sgrade, school ) 
                VALUES 
            <foreach collection="excelContent" item="item" separator=",">    
                (nextval('seq_test') , #{item.B}, #{item.C}, #{item.E} )
             </foreach>   
                
 	 </insert>


</mapper>